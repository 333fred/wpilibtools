<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="mainArea" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.wpi.first.wpilib.javainstaller.Controllers.MainController">
    <bottom>
        <HBox alignment="BOTTOM_RIGHT" spacing="10.0" BorderPane.alignment="BOTTOM_RIGHT">
            <children>
                <Button fx:id="button_cancel" mnemonicParsing="false" onAction="#handleCancelClick" text="Cancel"/>
                <Button fx:id="button_back" mnemonicParsing="false" text="Back"/>
                <Button fx:id="button_next" mnemonicParsing="false" text="Next"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </HBox>
    </bottom>
</BorderPane>
